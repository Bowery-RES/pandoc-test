{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:jasmine/recommended"
  ],
  "rules": {
    "no-console": ["error", { "allow": ["log", "warn", "error"] }],
    "no-param-reassign": ["error", { "props": false }]
  },
  "plugins": [
    "jasmine"
  ],
  "env": {
    "jasmine": true
  }
}
